# Camera
CONFIG_CUSTOM_KERNEL_IMGSENSOR="duchampov64b40wide_mipi_raw duchampov16a1qfront_mipi_raw duchampsc202pcsmacro_mipi_raw duchampov08d10ultra_mipi_raw"

# Charger
CONFIG_REGULATOR_WL2868C=m
CONFIG_RUST_DETECTION=y
CONFIG_XM_SINGLE_PD_MANAGER=m
CONFIG_XMEXT_SC858X_CHG_PUMP=m
CONFIG_XMEXT_TI_GAUGE=m

# Display
CONFIG_DRM_PANEL_N11A_42_02_0A_DSC_VDO=m
CONFIG_MI_DISP_CMDQ_SWT=y
CONFIG_MI_DISP_ESD_CHECK=y
CONFIG_MI_DISP_LHBM=y
CONFIG_MI_DISP_VDO_AOD=y

# Fingerprint
CONFIG_MTK_FINGERPRINT_SUPPORT=m
CONFIG_SPI_MT65XX=m
CONFIG_XIAOMI_FINGERPRINT=m

# Platform
CONFIG_BUILD_PRODUCT_NAME="duchamp"

# Sound
CONFIG_MTK_VOW_AMAZON_SUPPORT=n
CONFIG_MTK_VOW_GVA_SUPPORT=y
CONFIG_MTK_VOW_MAX_PDK_NUMBER=0
CONFIG_SND_SMARTPA_AW882XX=m
CONFIG_USB_SWITCH_ET7480=m
CONFIG_XLOGCHAR=m

# Thermal
CONFIG_MI_THERMAL_MULTI_CHARGE=y

# Touchscreen
CONFIG_TOUCHSCREEN_GOODIX_BRL_9916=m
CONFIG_TOUCHSCREEN_XIAOMI_TOUCHFEATURE=m

# Vibrator
CONFIG_SIH_VIBRATOR=m
